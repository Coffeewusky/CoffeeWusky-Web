<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Coffeewusky</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
      height: 100vh;
      margin: 0;
      background-color: #f5f5f5;
    }
    h1 {
      font-size: 3em;
      color: #333;
    }
    .socials {
      margin-top: 20px;
    }
    .socials a {
      margin: 0 10px;
      text-decoration: none;
      color: #1a73e8;
      font-size: 1.2em;
    }
  </style>
</head>
<body>
  <h1 id="title">Coffeewusky</h1>
  <div class="socials" id="socials"></div>

  <script>
    document.addEventListener("DOMContentLoaded", function() {
      const socials = [
        { name: "YouTube", url: "https://www.youtube.com/user/yourchannel" },
        { name: "Twitch", url: "https://www.twitch.tv/yourchannel" },
        { name: "Twitter", url: "https://twitter.com/yourhandle" }
      ];

      const socialsContainer = document.getElementById("socials");

      socials.forEach(social => {
        const anchor = document.createElement("a");
        anchor.href = social.url;
        anchor.textContent = social.name;
        anchor.target = "_blank";  // Open links in a new tab
        socialsContainer.appendChild(anchor);
      });
    });
  </script>
</body>
</html>
